#!/bin/bash
#x3.cert.
#x3.cert.
#x3.cert.
#x3.chain=auth,citizen,root
#x3.trust.domain=BE-AUTH
#x3.return.revocation.data=no
#x3.prefix=be.fedict.eid.prod.trust.revokedauthcertchain
#x3.url=https://trust-ws.services.belgium.be/eid-trust-service-ws/xkms2
#x3.delay=1000
#x3.expected.failure=RevocationStatus
#x3.out.filter=be\.fedict\.eid\.prod\.trust\.revokedauthcertchain\.(responsetime|result|result\.comment)$

